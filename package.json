    "lint-staged": "^13.2.2",
    "mini-css-extract-plugin": "^2.7.5",
    "postcss-loader": "^7.3.0",
    "prettier-standard": "^15.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-jsonschema-form-bs4": "^1.7.1",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.11.1",
