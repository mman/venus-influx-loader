    "@coreui/icons-react": "^2.1.0",
    "@coreui/react": "^4.8.0",
    "@popperjs/core": "^2.11.7",
    "autoprefixer": "^10.4.14",
    "axios-hooks": "^4.0.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-watch": "^7.7.2",
    "bootstrap": "^5.2.3",
